{
    "name": "kaile/yii2-unitegallery",
    "description": "Yii2 extension unitegallery for photos, videos and audio traks",
    "type": "yii2-extension",
    "keywords": ["yii2","extension","gallery","unitegallery"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mkhail Kornilov",
            "email": "fix-06@yandex.ru"
        }
    ],
    "require": {
        "yiisoft/yii2": "2.0.*"
    },
    "autoload": {
        "psr-4": {
            "kaile\\unitegallery\\": ""
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    }
}
